{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "src/DeDuBa/DeDuBa.csproj",
            "src/DeDuBa.Test/DeDuBa.Test.csproj",
            "src/Interfaces/Interfaces.csproj",
            "src/OsCallsCommon/OsCallsCommon.csproj",
            "src/OsCallsLinux/OsCallsLinux.csproj",
            "src/OsCallsWindows/OsCallsWindows.csproj",
            "src/UtilitiesLibrary/UtilitiesLibrary.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["index.md"]
      },
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "toc.yml"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      },
      {
        "files": ["doxygen/**"]
      }
    ],
    "dest": "_site"
  }
}
